    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.0",
    "framer-motion": "^6.5.1",
    "react": "^18.0.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "typescript": "^4.9.3",
